{
    "name": "laravel-flatpack",
    "version": "1.0.0",
    "description": "Flat pack: Administration panel for Laravel, ready to assemble.",
    "main": "tailwind.config.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/faustoq/laravel-flatpack.git"
    },
